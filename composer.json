        "filament/filament": "^3.0-stable",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Heloufir\\FilamentKanban\\": "src/"
        }
    },
    "authors": [
        {
            "name": "EL OUFIR Hatim",
            "role": "Developer",
            "email": "eloufirhatim@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
